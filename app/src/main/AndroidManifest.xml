<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.techfak.gse.dwenzel" >

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="dwenzel"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.ProjectTemplate" >
        <activity
            android:name=".start_screen.controller.GameStartActivity"
            android:exported="true"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".game_screen.controller.BoardMainActivity"
            android:exported="true"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".end_screen.EndGameActivity"
            android:exported="true"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>